 
     Scanner in = new Scanner (System.in);
     // length - quantas posição tem dentro do vetor
    int numero = 0;
    int fatorial = 1;
    System.out.println("DIGITE UM NUMERO");
    numero = in.nextInt();
    
 for (int i=numero; i>=1;i--){
     fatorial = fatorial*i;
 }
        System.out.println("fatorial de n é" + fatorial);
 
    }
}
  
